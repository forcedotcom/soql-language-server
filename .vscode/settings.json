{
  "license-header-manager.excludeExtensions": [
    ".sh",
    ".js",
    "**.test.**",
    "**.spect.**"
  ],
  "license-header-manager.excludeFolders": [
    "node_modules",
    "coverage",
    "dist",
    "docs",
    "scripts",
    "lib"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.vscode-test": true
  },
  "search.exclude": {
    "**/dist": true,
    "**/lib": true
  },
  "editor.tabSize": 2,
  "rewrap.wrappingColumn": 80,
  "editor.formatOnSave": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#b45151",
    "titleBar.activeBackground": "#b45151",
    "titleBar.activeForeground": "#f3f1ef"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
