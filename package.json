{
  "name": "@salesforce/soql-language-server",
  "version": "0.6.2",
  "description": "SOQL Language Server",
  "engines": {
    "node": "*"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc --project .",
    "clean": "rimraf lib && rimraf node_modules",
    "lint": "eslint src/",
    "test": "jest --runInBand",
    "test:unit:coverage": "jest --runInBand --coverage"
  },
  "dependencies": {
    "@salesforce/soql-common": "0.2.1",
    "antlr4-c3": "^1.1.13",
    "antlr4ts": "^0.5.0-alpha.3",
    "debounce": "^1.2.0",
    "vscode-languageclient": "6.1.3",
    "vscode-languageserver": "6.1.1",
    "vscode-languageserver-protocol": "3.15.3",
    "vscode-languageserver-textdocument": "1.0.1"
  },
  "resolutions": {
    "**/vscode-languageserver-protocol": "3.15.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/template": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@types/debounce": "^1.2.0",
    "@types/jest": "22.2.3",
    "@types/vscode": "1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "26.1.0",
    "jest-junit": "^12.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "26.1.3",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forcedotcom/soql-language-server.git"
  },
  "keywords": [
    "soql",
    "language-server",
    "lsp"
  ],
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn run lint"
    }
  }
}
